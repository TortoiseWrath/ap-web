RewriteEngine On
RewriteCond %{HTTP_HOST} ^(?:www\.)anonpress\.net$ [NC]
RewriteRule ^/in/?$ "https://anonpress.org/store/ap-index" [NC,R=301,L]
RewriteRule ^/camo/?$ "https://anonpress.org/store/camo" [NC,R=301,L]
RewriteRule ^/conc/?$ "https://anonpress.org/store/concordance" [NC,R=301,L]
RewriteRule ^/mi/?$ "https://anonpress.org/store/mini" [NC,R=301,L]
RewriteRule ^/soft/?$ "https://anonpress.org/store/soft" [NC,R=301,L]
RewriteRule ^/study/?$ "https://anonpress.org/store/study" [NC,R=301,L]
RewriteRule ^/wo/?$ "https://anonpress.org/store/wo" [NC,R=301,L]
RewriteRule ^/xl/?$ "https://anonpress.org/store/xl" [NC,R=301,L]
RewriteRule ^/e/?$ "https://anonpress.org/store/e-AA" [NC,R=301,L]
RewriteRule ^/view_cart.asp$ "https://anonpress.org/store/index.php?route=checkout/cart" [NC,R=301,L]
RewriteRule ^/myaccount.asp$ "https://anonpress.org/store/index.php?route=account/login" [NC,R=301,L]
RewriteRule ^/?$ "https://anonpress.org/store" [R=301,L]
